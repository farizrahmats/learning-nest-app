{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2021",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": false,
    "isolatedModules": false,
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "baseUrl": "./"
  }
}
